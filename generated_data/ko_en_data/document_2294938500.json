            {
                "order": 2,
                "start_time": "00:00:06",
                "end_time": "00:00:30",
                "content": "\uc774 \ub17c\ubb38\uc5d0\uc11c\ub294 \ub2a5\ub3d9\uc801 \ub3c5\uc11c, \ub178\ud2b8 \ud544\uae30, \uacf5\ubd80 \uadf8\ub8f9 \ub9cc\ub4e4\uae30 \ub4f1 \ud6a8\uc728\uc801\uc73c\ub85c \uacf5\ubd80\ud558\uae30 \uc704\ud55c \ub2e4\uc591\ud55c \uc804\ub7b5\uc744 \uc18c\uac1c\ud569\ub2c8\ub2e4.",
                "speaker": "\ub0b4\ub808\uc774\ud130"
            },
            {
                "order": 3,
                "start_time": "00:00:31",
                "end_time": "00:00:35",
                "content": "\ub610\ud55c \uc774 \ub17c\ubb38\uc5d0\uc11c\ub294 \ud559\uc5c5\uc801 \uc131\uacf5\uc5d0 \uc788\uc5b4\uc11c \ub3d9\uae30 \ubd80\uc5ec\uc640 \uc0ac\uace0\ubc29\uc2dd\uc758 \uc5ed\ud560\uc5d0 \ub300\ud574\uc11c\ub3c4 \ub17c\uc758\ud569\ub2c8\ub2e4.",
                "speaker": "\ub0b4\ub808\uc774\ud130"
            },
            {
                "order": 4,
                "start_time": "00:00:36",
                "end_time": "00:00:45",
                "content": "\uc774 \ub17c\ubb38\uc758 \ub9c8\uc9c0\ub9c9 \ubd80\ubd84\uc5d0\uc11c\ub294 \ud559\uc0dd\ub4e4\uc5d0\uac8c \uc788\uc5b4\uc11c \uc77c\uacfc \uc0b6\uc758 \uade0\ud615\uc774 \uc911\uc694\ud55c \uc774\uc720\uc5d0 \ub300\ud574 \ub2e4\ub8e8\uace0 \uc788\uc2b5\ub2c8\ub2e4.",
                "speaker": "\ub0b4\ub808\uc774\ud130"
            },
            {
                "order": 5,
                "start_time": "00:00:46",
                "end_time": "00:00:56",
                "content": "\ubcf8 \ub17c\ubb38\uc740 \uc8fc\uc694 \ub0b4\uc6a9\uc744 \uc694\uc57d\ud558\uace0 \ucd94\uac00 \ud559\uc2b5\uc744 \uc704\ud55c \ucd94\uac00 \uc790\ub8cc\ub97c \uc81c\uacf5\ud568\uc73c\ub85c\uc368 \uacb0\ub860\uc744 \ub9fa\uc2b5\ub2c8\ub2e4.",
                "speaker": "\ub0b4\ub808\uc774\ud130"
            }
