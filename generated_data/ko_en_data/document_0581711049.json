            },
            {
                "order": 3,
                "start_time": "00:00:31",
                "end_time": "00:00:45",
                "content": "\uc774 \uc139\uc158\uc5d0\uc11c\ub294 \ud78c\ub450\uad50\uc758 \uc8fc\uc694 \ucd95\uc81c\uc778 \ub514\uc648\ub9ac\uc758 \uc911\uc694\uc131\uc5d0 \ub300\ud574 \uc790\uc138\ud788 \uc54c\uc544\ubcf4\uaca0\uc2b5\ub2c8\ub2e4.",
                "speaker": "\ub0b4\ub808\uc774\ud130"
            },
            {
                "order": 4,
                "start_time": "00:00:46",
                "end_time": "00:00:59",
                "content": "\ub610\ud55c, \uc0c8\ub85c\uc6b4 \ud55c \ud574\uc758 \uc2dc\uc791\uc744 \uc54c\ub9ac\ub294 \uc911\uad6d \uc124\ub0a0\uc5d0 \ub300\ud574\uc11c\ub3c4 \ub17c\uc758\ud574\ubcf4\uaca0\uc2b5\ub2c8\ub2e4.",
                "speaker": "\ub0b4\ub808\uc774\ud130"
            },
            {
                "order": 5,
                "start_time": "00:01:00",
                "end_time": "00:01:15",
                "content": "\ub9c8\uc9c0\ub9c9\uc73c\ub85c \uc11c\uc591 \ubb38\ud654\uc640 \uc804\ud1b5\uc5d0\uc11c \ud06c\ub9ac\uc2a4\ub9c8\uc2a4\uc758 \uc758\ubbf8\ub97c \uc0b4\ud3b4\ubcf4\uaca0\uc2b5\ub2c8\ub2e4.",
                "speaker": "\ub0b4\ub808\uc774\ud130"
            },
            {
                "order": 6,
                "start_time": "00:01:16",
                "end_time": "00:01:30",
                "content": "\uc774 \uc5f0\uad6c\uc758 \uc8fc\uc694 \uacb0\uacfc\uc640 \uad50\ud6c8\uc744 \uac04\ub7b5\ud558\uac8c \uc694\uc57d\ud558\uba74\uc11c \ub9c8\ubb34\ub9ac\ud558\uaca0\uc2b5\ub2c8\ub2e4.",
                "speaker": "\ub0b4\ub808\uc774\ud130"
            }
