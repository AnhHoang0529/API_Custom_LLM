    "desc": "\uc774 \uc5f0\uad6c \ub17c\ubb38\uc5d0\uc11c\ub294 \uc804 \uc138\uacc4 \ub2e4\uc591\ud55c \uc57c\uc0dd \ub3d9\ubb3c\uc758 \ud589\ub3d9\uacfc \uc11c\uc2dd\uc9c0\ub97c \ud0d0\uad6c\ud569\ub2c8\ub2e4.",
    "narrationStt": {
        "sttData": [
            {
                "order": 1,
                "start_time": "00:00:00",
                "end_time": "00:00:05",
                "content": "\uc57c\uc0dd \ub3d9\ubb3c\uacfc \uadf8 \uc11c\uc2dd\uc9c0\uc5d0 \uad00\ud55c \uc5f0\uad6c \ub17c\ubb38\uc5d0 \uc624\uc2e0 \uac83\uc744 \ud658\uc601\ud569\ub2c8\ub2e4.",
                "speaker": "\ub0b4\ub808\uc774\ud130"
            },
            {
                "order": 2,
                "start_time": "00:00:05",
                "end_time": "00:00:15",
                "content": "\uc774 \ub17c\ubb38\uc5d0\uc11c\ub294 \uc57c\uc0dd \ub3d9\ubb3c \ubcf4\ud638\uc758 \ub2e4\uc591\ud55c \uce21\uba74\uacfc \uc790\uc5f0 \uc11c\uc2dd\uc9c0\ub97c \ubcf4\uc874\ud558\ub294 \ub370 \uc9c1\uba74\ud55c \uacfc\uc81c\uc5d0 \ub300\ud574 \ub17c\uc758\ud569\ub2c8\ub2e4.",
                "speaker": "\ub0b4\ub808\uc774\ud130"
            },
            {
                "order": 3,
                "start_time": "00:00:15",
                "end_time": "00:00:18",
                "content": "\uba3c\uc800, \ub2e4\uc591\ud55c \uc9c0\uc5ed\uc5d0\uc11c \ubc1c\uacac\ub418\ub294 \uac00\uc7a5 \ub9e4\ud639\uc801\uc778 \uc57c\uc0dd \ub3d9\ubb3c \uc885\uc744 \uc0b4\ud3b4\ubcf4\uaca0\uc2b5\ub2c8\ub2e4.",
                "speaker": "\ub0b4\ub808\uc774\ud130"
            },
            {
                "order": 4,
                "start_time": "00:00:18",
                "end_time": "00:00:35",
                "content": "\uc544\ud504\ub9ac\uce74 \uc0ac\ubc14\ub098\uc5d0\ub294 \uc138\uacc4\uc5d0\uc11c \uac00\uc7a5 \ud070 \uc721\uc9c0 \ub3d9\ubb3c, \uc704\uc5c4 \uc788\ub294 \uc544\ud504\ub9ac\uce74\ucf54\ub07c\ub9ac\uac00 \uc788\uc2b5\ub2c8\ub2e4.",
                "speaker": "\ub0b4\ub808\uc774\ud130"
            },
            {
                "order": 5,
                "start_time": "00:00:35",
                "end_time": "00:00:40",
                "content": "\ub2e4\uc74c\uc73c\ub85c \uc5bc\uc74c\uc73c\ub85c \ub4a4\ub36e\uc778 \uadf9\uc9c0\ubc29\uc744 \ubc29\ubb38\ud558\uc5ec \ubd81\uadf9\uacf0\uacfc \uadf9\ud55c \uae30\uc628\uc5d0 \ub300\ud55c \uadf8\ub4e4\uc758 \uc801\uc751\ub825\uc5d0 \ub300\ud574 \uc54c\uc544\ubcf4\uaca0\uc2b5\ub2c8\ub2e4.",
                "speaker": "\ub0b4\ub808\uc774\ud130"
            },
            {
                "order": 6,
                "start_time": "00:00:40",
                "end_time": "00:00:50",
                "content": "\ub098\uc911\uc5d0 \uc6b0\ub9ac\ub294 \uc218\ub9ce\uc740 \uc774\uad6d\uc801\uc778 \uc885\ub4e4\uc774 \uc11c\uc2dd\ud558\ub294 \uc544\ub9c8\uc874 \uc5f4\ub300 \uc6b0\ub9bc\uc758 \uae4a\uc740 \uacf3\uc73c\ub85c \ub4e4\uc5b4\uac00\ubcf4\uaca0\uc2b5\ub2c8\ub2e4.",
                "speaker": "\ub0b4\ub808\uc774\ud130"
            },
            {
                "order": 7,
                "start_time": "00:00:50",
                "end_time": "00:00:55",
                "content": "\ub9c8\uc9c0\ub9c9\uc73c\ub85c \uc57c\uc0dd \ub3d9\ubb3c \ubcf4\ud638 \ud65c\ub3d9\uc758 \uc911\uc694\uc131\uacfc \uc0dd\ubb3c \ub2e4\uc591\uc131 \ubcf4\uc874\uc5d0 \ubbf8\uce58\ub294 \uc601\ud5a5\uc744 \uc0b4\ud3b4\ubcf4\uaca0\uc2b5\ub2c8\ub2e4.",
                "speaker": "\ub0b4\ub808\uc774\ud130"
            }
