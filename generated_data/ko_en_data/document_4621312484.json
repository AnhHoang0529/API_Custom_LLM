    "desc": "\ub2e4\uc591\ud55c \ud559\uc2b5 \uae30\ubc95\uacfc \uc5c5\ubb34 \uc811\uadfc \ubc29\uc2dd\uc758 \ud6a8\uacfc\uc5d0 \ub300\ud55c \ud3ec\uad04\uc801\uc778 \uc5f0\uad6c \ub17c\ubb38\uc785\ub2c8\ub2e4.",
    "narrationStt": {
        "sttData": [
            {
                "order": 1,
                "start_time": "00:00:00",
                "end_time": "00:00:15",
                "content": "\ucd5c\uadfc \ub4e4\uc5b4, \uacf5\ubd80\uc640 \uc5c5\ubb34\uc5d0 \uac00\uc7a5 \ud6a8\uacfc\uc801\uc778 \ubc29\ubc95\uc744 \uc774\ud574\ud558\ub824\ub294 \uad00\uc2ec\uc774 \uc810\uc810 \ucee4\uc9c0\uace0 \uc788\uc2b5\ub2c8\ub2e4.",
                "speaker": "\ub0b4\ub808\uc774\ud130"
            },
            {
                "order": 2,
                "start_time": "00:00:15",
                "end_time": "00:00:30",
                "content": "\uc774 \uc5f0\uad6c \ub17c\ubb38\uc5d0\uc11c\ub294 \ub2e4\uc591\ud55c \ud559\uc2b5 \uae30\uc220\uacfc \uc791\uc5c5 \uc804\ub7b5\uc744 \ud0d0\uad6c\ud569\ub2c8\ub2e4.",
                "speaker": "\ub0b4\ub808\uc774\ud130"
            },
            {
                "order": 3,
                "start_time": "00:00:30",
                "end_time": "00:00:45",
                "content": "\uba3c\uc800 \ud3ec\ubaa8\ub3c4\ub85c \ud14c\ud06c\ub2c9\uc744 \uc0b4\ud3b4\ubcf4\uaca0\uc2b5\ub2c8\ub2e4. \ud3ec\ubaa8\ub3c4\ub85c \ud14c\ud06c\ub2c9\uc740 \uc9d1\uc911\uc801\uc778 \uc791\uc5c5\uc744 \ud55c \ub2e4\uc74c \uc9e7\uc740 \ud734\uc2dd\uc744 \ucde8\ud558\ub294 \ubc29\uc2dd\uc785\ub2c8\ub2e4.",
                "speaker": "\ub0b4\ub808\uc774\ud130"
            },
            {
                "order": 4,
                "start_time": "00:00:45",
                "end_time": "00:01:00",
                "content": "\uc774\uc5b4\uc11c \uae30\uc5b5\uc220\uacfc \ubd84\uc0b0 \ubc18\ubcf5\uc758 \uc0ac\uc6a9\uc5d0 \ub300\ud574 \ub17c\uc758\ud574 \ubcf4\uaca0\uc2b5\ub2c8\ub2e4.",
                "speaker": "\ub0b4\ub808\uc774\ud130"
            },
            {
                "order": 5,
                "start_time": "00:01:00",
                "end_time": "00:01:15",
                "content": "\ub9c8\uc9c0\ub9c9\uc73c\ub85c, \ub2e4\uc591\ud55c \uc791\uc5c5 \ud658\uacbd\uc774 \uc0dd\uc0b0\uc131\uc5d0 \ubbf8\uce58\ub294 \uc601\ud5a5\uc744 \ubd84\uc11d\ud569\ub2c8\ub2e4.",
                "speaker": "\ub0b4\ub808\uc774\ud130"
            },
            {
                "order": 6,
                "start_time": "00:01:15",
                "end_time": "00:01:30",
                "content": "\uc774 \uc5f0\uad6c\ub294 \ud559\uc0dd\uacfc \uc804\ubb38\uac00 \ubaa8\ub450\uc5d0\uac8c \uadc0\uc911\ud55c \ud1b5\ucc30\ub825\uc744 \uc81c\uacf5\ud569\ub2c8\ub2e4.",
                "speaker": "\ub0b4\ub808\uc774\ud130"
            }
        ],
        "id": 62332
    },
    "category": {
        "id": 131,
